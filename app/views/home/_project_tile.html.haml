.col-sm-6
  = link_to project_tile do
    .thumbnail
      = image_tag(project_tile.logo)
      .caption.text-center
        %h2= project_tile.name
      .text-left.pull-left
        %span.label.label-info.label-big= project_tile.map_marker.try(:city)
      .text-right.pull-right
        = stars_for_rating(5)
      .clearfix
